# Supabase Configuration (Public - Browser access)
# Get these values from your Supabase project dashboard
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase Configuration (Private - Server-only)
# Get the Service Role Key from your Supabase project settings -> API
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# App URL (used for auth redirects and cookies)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Vercel Analytics Token (optional, for production monitoring)
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=
